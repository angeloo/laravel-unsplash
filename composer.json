{
    "name": "xchimx/laravel-unsplash",
    "description": "Laravel package for easy integration with the Unsplash API. It allows you to use the Unsplash API in your Laravel applications to fetch photos, collections, and user data.",
    "keywords": [
        "laravel",
        "laravel-unsplash",
        "unsplash",
        "schottstaedt.net",
        "Tobias Schottstädt",
        "api",
        "photos",
        "images"
    ],
    "license": "MIT",
    "type": "library",
    "homepage": "https://www.schottstaedt.net",
    "authors": [
        {
            "name": "Tobias Schottstädt",
            "email": "code@schottstaedt.net",
            "homepage": "https://www.schottstaedt.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Xchimx\\UnsplashApi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xchimx\\UnsplashApi\\UnsplashServiceProvider"
            ],
            "aliases": {
                "Unsplash": "Xchimx\\UnsplashApi\\Facades\\Unsplash"
            }
        }
    },
    "support": {
        "issues": "https://github.com/xchimx/laravel-unsplash/issues",
        "source": "https://github.com/xchimx/laravel-unsplash"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
